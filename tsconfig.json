{
	"compilerOptions": {
		"strictNullChecks": true,
		"moduleResolution": "node",
		"module": "esnext",
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"noImplicitAny": true,
		"target": "es2017",
		"removeComments": true,
		"jsx": "react",
		// "jsx": "preserve",
		"sourceMap": true
	},
	"exclude": [
		"node_modules",
	]
}
